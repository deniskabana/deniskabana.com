{
  "name": "deniskabana.com",
  "version": "0.10.0",
  "scripts": {
    "build": "next export",
    "clean:dist": "rm -rf ./out",
    "clean:hard": "yarn clean:soft && rm -rf ./.yarn ./yarn.lock ./tsconfig.tsbuildinfo ./next-env.d.ts",
    "clean:soft": "yarn clean:dist && rm -rf ./node_modules ./next",
    "code:lint": "next lint",
    "code:prettier": "prettier --write --log-level=silent",
    "code:sortpkg": "sort-package-json",
    "code:stylelint": "stylelint",
    "code:tsc": "tsc",
    "dev": "next dev",
    "health:check": "yarn code:tsc && yarn code:lint && yarn code:stylelint ./src/**/*.{ts,tsx}",
    "health:fix": "yarn code:prettier ./src/**/*.{json,md,html} && yarn code:lint --fix && yarn code:sortpkg && yarn code:stylelint --fix ./src/**/*.{ts,tsx}",
    "prepare": "husky install",
    "start": "yarn clean:dist && yarn build && cd out && npx serve -l 3000"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@mui/icons-material": "^5.14.7",
    "@mui/lab": "^5.0.0-alpha.150",
    "@mui/material": "^5.14.7",
    "@tanstack/react-query": "^4.33.0",
    "axios": "^1.5.1",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.19",
    "i18next": "^23.5.1",
    "next": "13.4.19",
    "next-i18next": "^14.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^13.2.2",
    "react-persistent-state-hook": "latest",
    "typescript": "5.1"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@types/node": "20.5.4",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^3.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.29",
    "postcss-styled-syntax": "^0.4.0",
    "prettier": "^3.0.3",
    "sort-package-json": "^2.5.1",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "ts-node": "^10.9.1"
  },
  "packageManager": "yarn@3.6.3",
  "engines": {
    "node": ">=20.5.1",
    "yarn": ">=3.6.1"
  }
}
